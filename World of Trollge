Tab:AddButton({
	Name = "Destroy GUI",
	Callback = function()
      		OrionLib:Destroy()
  	end    
})

Tab2:AddToggle({
                    Name = "Auto Collect Cups/Relics",
                    Default = false,
                    Callback = function(Value)
AutoCups = Value
while AutoCups do
for i, v in ipairs(game.Workspace:GetChildren()) do
                if v:IsA("Tool") and v:FindFirstChild("Pick up",true) and v.Name ~= "delicous!!" then
                if v.Handle.Anchored == false then
game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                end
            end
            end
game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
            task.wait()
            end
end
})

Tab2:AddToggle({
                    Name = "Quick Craft Cups",
                    Default = false,
                    Callback = function(Value)
AutoCraft = Value
if AutoCraft == true then
OGL = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
while AutoCraft do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-360,27,-322)
for i, v in ipairs(workspace.Map.GreenLevel.Spawn.ForgingCenter.Forging:GetDescendants()) do
                if v.ClassName == "Part" and v:FindFirstChild("ProximityPrompt") then
fireproximityprompt(v.ProximityPrompt,0)
                end
            end
task.wait()
end
end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OGL
end
})

Tab2:AddDropdown({
	Name = "TP to cup spot",
	Default = "1",
	Options = {"Water Cup", "Hearty Cup", "Super Oil", "Fighting Spirit", "Empty Cup", "Flared essence", "Dark Orb", "sploinky cup", "Oil, blood, sorrow, night's essence, and flaming oil cup"},
	Callback = function(Value)
if Value == "Empty Cup" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.GreenLevel.Spawn.ForgingCenter["The hole"].Part.CFrame
elseif Value == "Water Cup" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Red tree dimension"].Hub.WaterObby.Water["Water cup"].CFrame
elseif Value == "Hearty Cup" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Red tree dimension"].Hub.HeartyArea.HeartyStand["Parts - Neon"].Part.CFrame
elseif Value == "Oil, blood, sorrow, night's essence, and flaming oil cup" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-360,27,-322)
elseif Value == "Flared essence" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.YellowLevelRight.MISC.FlaredEssenceAltar.NewAltar.ProximityPromptPart.CFrame
elseif Value == "Dark Orb" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.BlueLevel.MISC.DarkAltar.DarkOrbAltar.ProximityPromptPart.CFrame
elseif Value == "Super Oil" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Oil-man's Base"].anvil.Part.CFrame
elseif Value == "sploinky cup" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.GreenLevel.Buildings["Ki-Bank"].Toilet.CFrame
elseif Value == "Fighting Spirit" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Red tree dimension"].Hub.BlueTreeArea.Statue.Part.CFrame
end
	end    
})

Tab3:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(140, 185, 255),
	Increment = 1,
	ValueName = "WS",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end    
})

Tab3:AddToggle({
	Name = "Keep Walkspeed",
	Default = false,
	Callback = function(Value)
KeepWalkspeed = Value
            while KeepWalkspeed do
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") ~= nil and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Walkspeed then
                    game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed = Walkspeed
                end
task.wait()
            end
	end    
})

Tab3:AddSlider({
	Name = "Jumppower",
	Min = 50,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255, 185, 140),
	Increment = 1,
	ValueName = "JP",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
Jumppower = Value
	end    
})

Tab3:AddToggle({
	Name = "Keep Jumppower",
	Default = false,
	Callback = function(Value)
KeepJumppower = Value
            while KeepJumppower do
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") ~= nil and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Jumppower then
                    game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpPower = Jumppower
                end
task.wait()
            end
	end    
})
end
for i,v in pairs(gethui().Orion:GetDescendants()) do
                    if v.ClassName == "Frame" and v.BackgroundTransparency < 0.3 then
v.BackgroundTransparency = 0.05
                    end
                end
for i,v in pairs(gethui().Orion:GetDescendants()) do
                    if v.ClassName == "Frame" and v.BackgroundTransparency < 0.3 and v.BackgroundColor3 == Color3.fromRGB(32, 32, 42) then
v.BackgroundTransparency = 1
                    end
                end
gethui().Orion.Name = "OrionEdited"
