local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Mobile%20Friendly%20Orion')))()

local Window = OrionLib:MakeWindow({Name = "tower defense knockoff", HidePremium = true, IntroEnabled = false, SaveConfig = false, ConfigFolder = "OrionTest"})

Tab2:AddToggle({
                    Name = "Slap Aura",
                    Default = false,
                    Callback = function(Value)
AntiNull = Value
while AntiNull do
for i,v in pairs(game.Workspace.Maps[game.Players.LocalPlayer.Name.."'s Map"]:GetDescendants()) do
                    if v.Name == "Head" then
game.ReplicatedStorage.b:FireServer(v)
end
end
task.wait()
end
end
                })
