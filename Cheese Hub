local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Cheese Hub", "Synapse")
local Tab = Window:NewTab("Scripts")
local Section = Tab:NewSection("Scripts")
Section:NewButton("Energy", "Makes your character cool.", function()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
    if v:IsA("Accessory") then
        if v.Handle:FindFirstChildOfClass("Mesh") or v.Handle:FindFirstChildOfClass("SpecialMesh") then
            if v.Handle:FindFirstChildOfClass("SpecialMesh") then
                v.Handle:FindFirstChildOfClass("SpecialMesh"):Remove()
            end
        end
    end
end
loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Powers'))()
end)
Section:NewButton("Plane", "Gives your character a plane.", function()
game:GetService("Players").LocalPlayer.Character["MeshPartAccessory"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["PirateCaptain_HatAccessory"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["WDW_FoamFinger"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["RCap"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Kate Hair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["LavanderHair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Pal Hair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Hat1"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["AFunRead"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Plane'))()
end)
Section:NewButton("Mini Man", "Makes your character a mini man.", function()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
    if v:IsA("Accessory") then
        if v.Handle:FindFirstChildOfClass("Mesh") or v.Handle:FindFirstChildOfClass("SpecialMesh") then
            if v.Handle:FindFirstChildOfClass("SpecialMesh") then
                v.Handle:FindFirstChildOfClass("SpecialMesh"):Remove()
            end
        end
    end
end
loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Mini%20Man'))()
end)
Section:NewButton("PC", "Gives your character a pc.", function()
game:GetService("Players").LocalPlayer.Character["PirateCaptain_HatAccessory"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["WDW_FoamFinger"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["RCap"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Kate Hair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["LavanderHair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/PC'))()
end)
Section:NewButton("SCP-049", "Makes your character a plaque doctor.", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/SCP-049'))()
end)
Section:NewButton("Thruster", "Gives your character a thruster.", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
game:GetService("Players").LocalPlayer.Character["LavanderHair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["PirateCaptain_HatAccessory"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["MeshPartAccessory"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Pal Hair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Hat1"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
game:GetService("Players").LocalPlayer.Character["Kate Hair"].Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Thruster'))()
end)
